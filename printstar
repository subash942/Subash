import java.net.StandardSocketOptions;
import java.util.Scanner;
public class star{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n =sc.nextInt();
        int m =sc.nextInt();
        /*..outer...*/
        for(int i=1 ; i<=n; i++){
            /*...inner...*/
            for(int j=1; j<=m; j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
